{
  "expo": {
    "name": "EatMe",
    "slug": "EatMe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/App_Logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/logo_01.png",
      "resizeMode": "native",
      "backgroundColor": "#F5F5F6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/App_Logo.png",
        "backgroundColor": "#fc6c44"
      },
      "package": "com.chronstorm.FoodSellApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
